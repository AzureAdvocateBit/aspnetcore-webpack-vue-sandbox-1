{
  "compilerOptions": {
    "outDir": "../wwwroot",
    "noImplicitAny": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noEmitOnError": false,
    "sourceMap": true,
    "allowJs": true,
    "checkJs": true,
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node"
  },
  "include": [
    "**/*.js",
    "**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "types": [
    "node"
  ]
}